journal = {Scientific Reports},
keywords = {Perception,Sensory processing},
month = {dec},
number = {1},
pages = {14842},
publisher = {Nature Publishing Group},
title = {{Cardio-audio synchronization drives neural surprise response}},
url = {http://www.nature.com/articles/s41598-017-13861-8},
volume = {7},
year = {2017}
}
@article{Beauchaine2001,
