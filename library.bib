file = {:C$\backslash$:/Users/hodos/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Garca, Romo, Figueroa-Nazuno - 2013 - Characterization of ground motions using recurrence plots.pdf:pdf},
issn = {00167169},
