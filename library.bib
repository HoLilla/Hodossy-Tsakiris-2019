@article{Lowe2016,
abstract = {Joint Action is typically described as social interaction that requires coordination among two or more co-actors in order to achieve a common goal. In this article, we put forward a hypothesis for the existence of a neural-computational mechanism of affective valuation that may be critically exploited in Joint Action. Such a mechanism would serve to facilitate coordination between co-actors permitting a reduction of required information. Our hypothesized affective mechanism provides a value function based implementation of Associative Two-Process (ATP) theory that entails the classification of external stimuli according to outcome expectancies. This approach has been used to describe animal and human action that concerns differential outcome expectancies. Until now it has not been applied to social interaction. We describe our Affective ATP model as applied to social learning consistent with an “extended common currency” perspective in the social neuroscience literature. We contrast this to an alternative mechanism that provides an example implementation of the so-called social-specific value perspective. In brief, our Social-Affective ATP mechanism builds upon established formalisms for reinforcement learning (temporal difference learning models) nuanced to accommodate expectations (consistent with ATP theory) and extended to integrate non-social and social cues for use in Joint Action.},
author = {Lowe, R and Alm{\'{e}}r, A and Lindblad, G and Gander, P and Michael, J and Vesper, C},
doi = {10.3389/fncom.2016.00088},
file = {:C$\backslash$:/Users/hodos/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Lowe et al. - 2016 - Minimalist social-affective value for use in joint action A neural-computational hypothesis.pdf:pdf},
issn = {1662-5188},
journal = {Frontiers in Computational Neuroscience},
keywords = {classification,expectancy,expectation,human,human versus animal comparison,joint,nervous system,neuroscience,process model,reinforcement,social interaction,social learning,stimulus,theoretical model},
number = {AUG},
pages = {1--18},
title = {{Minimalist social-affective value for use in joint action: A neural-computational hypothesis}},
url = {http://www.embase.com/search/results?subaction=viewrecord{\&}from=export{\&}id=L611800845{\%}5Cnhttp://dx.doi.org/10.3389/fncom.2016.00088{\%}5Cnhttp://sfx.library.uu.nl/utrecht?sid=EMBASE{\&}issn=16625188{\&}id=doi:10.3389/fncom.2016.00088{\&}atitle=Minimalist+social-affecti},
