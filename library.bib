journal = {Physiological reviews},
number = {2},
pages = {453--79},
pmid = {19342612},
title = {{Brain basis of human social interaction: from concepts to brain imaging.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19342612},
volume = {89},
year = {2009}
}
