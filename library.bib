@article{Tsakiris2011b,
abstract = {Body-awareness relies on the representation of both interoceptive and exteroceptive percepts coming from one's body. However, the exact relationship and possible interaction of interoceptive and exteroceptive systems for body-awareness remain unknown. We sought to understand for the first time, to our knowledge, the interaction between interoceptive and exteroceptive awareness of the body. First, we measured interoceptive awareness with an established heartbeat monitoring task. We, then, used a multi-sensory-induced manipulation of body-ownership (e.g. Rubber Hand Illusion (RHI)) and we quantified the extent to which participants experienced ownership over a foreign body-part using behavioural, physiological and introspective measures. The results suggest that interoceptive sensitivity predicts the malleability of body representations, that is, people with low interoceptive sensitivity experienced a stronger illusion of ownership in the RHI. Importantly, this effect was not simply owing to a poor proprioceptive representation or differences in autonomic states of one's body prior to the multi-sensory stimulation, suggesting that interoceptive awareness modulates the online integration of multi-sensory body-percepts.},
author = {Tsakiris, Manos and Tajadura-Jim{\'{e}}nez, Ana and Costantini, Marcello},
doi = {10.1098/rspb.2010.2547},
file = {::},
isbn = {1471-2954 (Electronic) 0962-8452 (Linking)},
issn = {09628452},
journal = {Proceedings of the Royal Society B: Biological Sciences},
keywords = {Body-awareness,Interoception,Multi-sensory},
month = {aug},
number = {1717},
pages = {2470--2476},
pmid = {21208964},
publisher = {The Royal Society},
title = {{Just a heartbeat away from one's body:Interoceptive sensitivity predicts malleability of body-representations}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21208964 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3125630},
volume = {278},
year = {2011}
}
