keywords = {Aversive conditioning,Reappraisal,Rescorla Wagner rule,Ventral tegmental area},
pages = {138--148},
title = {{Cognitive emotion regulation enhances aversive prediction error activity while reducing emotional responses}},
volume = {123},
year = {2015}
}
