keywords = {emotion perception,emotion regulation,empathy,mentalizing,social cognition,social support},
title = {{Special Focus on Emotion The Social Regulation of Emotion: An Integrative, Cross-Disciplinary Model}},
url = {http://ac.els-cdn.com/S1364661315002272/1-s2.0-S1364661315002272-main.pdf?{\_}tid=6b722896-47a1-11e7-b9c6-00000aab0f6c{\&}acdnat=1496414607{\_}bfe77439185fca7bf66285f4984775ec},
